{
  "alias": "flashblaze.xyz",
  "build": {
    "env": {
      "API_KEY": "@fashfb-api",
      "AUTH_DOMAIN": "@flashfb-auth",
      "DATABASE_URL": "@flashfb-database",
      "PROJECT_ID": "@flashfb-projectid",
      "STORAGE_BUCKET": "@flashfb-storagebucket",
      "MESSAGING_SENDER_ID": "@flashfb-messagingsenderid",
      "APP_ID": "@flashfb-appid",
      "MEASUREMENT_ID": "@flashfb-measurementid"
    }
  },
  "redirects": [
    {
      "source": "/js/index.js",
      "destination": "https://plausible.io/js/plausible.js"
    },
    {
      "source": "/api/event",
      "destination": "https://plausible.io/api/event"
    }
  ]
}
